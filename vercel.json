{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/email/newsletter",
      "destination": "/api/email/newsletter"
    },
    {
      "source": "/api/email/contact",
      "destination": "/api/email/contact"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
